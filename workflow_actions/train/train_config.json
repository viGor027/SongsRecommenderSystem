{
  "run_single_training_config": {
    "architecture": {
      "weights": "IMAGENET1K_V1",
      "class_name": "ResNetAssembly",
      "classifier": {
        "n_classes": 110,
        "sigmoid_output": true,
        "n_classifier_layers": 1,
        "classifier_activation": null,
        "n_units_per_classifier_layer": []
      },
      "backbone_name": "resnet101",
      "freeze_backbone": false,
      "sequence_encoder": {
        "n_embedding_dims": 384,
        "n_seq_encoder_layers": 1,
        "seq_encoder_activation": null,
        "n_units_per_seq_encoder_layer": []
      }
    },
    "dataloaders": {
      "drop_last": false,
      "batch_size": 64,
      "pin_memory": true,
      "num_workers": 12,
      "dataset_type": "augmented_dataset",
