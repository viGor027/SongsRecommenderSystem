{
  "run_single_training_config": {
    "architecture": {
      "class_name": "CnnDenseAssembly",
      "temporal_compressor":{
        "ConvCls": "Conv2DBlockWithSkip",
        "n_blocks": 5,
        "n_input_channels": 1,
        "n_layers_per_block": [1, 1, 1, 1, 1],
        "n_filters_per_block": [64, 64, 64, 64, 64],
        "n_filters_per_skip": [32, 32, 32, 32, 32],
        "conv_activation": "hardswish",
        "reduction_strat": "conv"
      },
      "sequence_encoder": {
        "n_seq_encoder_layers": 3,
        "n_units_per_seq_encoder_layer": [448, 448],
        "seq_encoder_activation": "hardswish",
        "n_embedding_dims": 384
      },
      "classifier": {
        "n_classifier_layers": 1,
        "n_units_per_classifier_layer": [],
        "n_classes": 110,
        "classifier_activation": null,
        "sigmoid_output": true
      }
    }
    ,
    "dataloaders": {
      "batch_size": 2,
      "num_workers": 2,
      "persistent_workers": true,
      "prefetch_factor": 20,
      "pin_memory": true,
      "drop_last": false
    },
    "hparams": {
      "learning_rate": 1e-4,
      "epochs": 100
    },
    "early_stopping": {
      "monitor": "val_loss",
      "mode": "min",
      "patience": 4,
      "verbose": true
    },
    "accelerator": "auto",
    "precision": "bf16-true",
    "project": "WB_tests",
    "run_name": "SPPED_TEST_REMOTE_bf16_true",
    "do_pre_epoch_hook": false
  }
}