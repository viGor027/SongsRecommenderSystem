{
  "run_single_training_config": {
    "architecture": {
      "class_name": "VAEAssembly",
      "sequence_encoder": {
          "n_seq_encoder_layers": 4,
          "n_units_per_seq_encoder_layer": [512, 512, 512],
          "seq_encoder_activation": "relu",
          "n_embedding_dims": 384
      },
      "classifier": {
        "n_classifier_layers": 1,
        "n_units_per_classifier_layer": [],
        "n_classes": 110,
        "classifier_activation": null,
        "sigmoid_output": true
      }
    }
    ,
    "dataloaders": {
      "dataset_type": "fragments_dataset",
      "batch_size": 1,
      "num_workers": 1,
      "persistent_workers": true,
      "prefetch_factor": 2,
      "pin_memory": true,
      "drop_last": false
    },
    "hparams": {
      "learning_rate": 1e-4,
      "epochs": 100
    },
    "early_stopping": {
      "monitor": "val_loss",
      "mode": "min",
      "patience": 5,
      "verbose": true
    },
    "accelerator": "auto",
    "precision": "bf16-true",
    "project": "WB_tests",
    "run_name": "SPPED_TEST_STACJO_bf16_true",
    "do_pre_epoch_hook": false
  }
}