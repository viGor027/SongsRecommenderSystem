{
  "raw_augment": {
    "log_path": null,
    "augmentations": [
      {
        "name": "AddGaussianNoise",
        "params": {
          "min_amplitude": 0.001,
          "max_amplitude": 0.015,
          "p": 0.1
        }
      },
      {
        "name": "Mp3Compression",
        "params": {
          "min_bitrate": 16,
          "max_bitrate": 96,
          "backend": "fast-mp3-augment",
          "preserve_delay": false,
          "p": 0.9
        }
      }
    ]
  },
  "spectrogram_augment": {
      "log_path": null,
      "augmentations": [
        {
          "name": "TimeMasking",
          "params": {
            "time_mask_param": 80,
            "iid_masks": false,
            "p": 0.35
          }
        },
        {
          "name": "FrequencyMasking",
          "params": {
            "freq_mask_param": 30,
            "iid_masks": false
          }
        }
      ]
    },
  "chunker": {
    "song_type": "numpy",
    "sample_rate": 22050,
    "validation_probability": 0.08,
    "n_seconds": 5,
    "step": 1
  },
  "spectrogram_extractor": {
    "extraction_method":"mel",
    "sample_rate": 22050,
    "n_mels": 80
  }
}
